<div class="container">
    <h1 class="mt-3 mb-3"> Danh sách yêu thích</h1>
    <table class="table">
    <thead>
        <tr>
        <th scope="col">#</th>
        <th scope="col">Tên sách</th>
        <th scope="col">Thể loại</th>
        <th scope="col">Tác giả</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <th scope="row"></th>
            <td>{{this.name}}</td>
            <td>{{this.typeID}}</td>
            <td>{{this.authoID}}</td>
        </tr>
        {{else}}
        <tr>
            <th scope="row"></th>
            <td>Không có sách yêu thích.</td>
            <td></td>
            <td></td>
        </tr>
        {{/each}}
    </tbody>
    </table>
</div>