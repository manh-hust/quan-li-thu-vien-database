<div class="container home-container tutorial-container">
  <div class="home-content tutorial-content">
      <div class="home-news tutorial-left">
        <h4>TIN TỨC</h4>
        <ul class="ms-2 mt-2">
          {{#each posts}}
          <li><a href="http://localhost:3000/news">{{this.title}}</a></li>
          {{/each}}
        </ul>
      </div>
      <div class="home-activity tutorial-right">
        <div class="list-tutorial">
          <div class="item-tutorial" id="borrow">
            <h3 class="item-title-tutorial">Thông tin hoạt động - Nghiệp vụ</h3>
            <ul>
                {{#each posts}}
                <li class="news-list">
                  <div class="container-img">
                    <img src='{{this.url}}' alt="image">
                  </div>
                  <div class="container-content">
                    <a href="">{{this.title}}</a>
                    <p>{{this.content}}</p>
                    <p class="date-news">{{this.date}}</p>
                  </div>
                </li>
                {{/each}}
            </ul>
          </div>
        </div>
      </div>
  </div>
</div>